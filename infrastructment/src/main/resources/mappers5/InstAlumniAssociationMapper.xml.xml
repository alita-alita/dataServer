<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.idicc.taotie.infrastructment.mapper.xiaoai.InstAlumniAssociationMapper">


    <select id="getRefAcademicByUniCode" parameterType="map"
            resultType="cn.idicc.taotie.infrastructment.entity.xiaoai.InstAlumniAssociationDO">

        select t1.* from inst_alumni_association t1
            left join inst_alumni_association_member t2 on t1.alumni_md5  = t2.alumni_md5
        where t2.member_uni_code = #{uniCode}

    </select>
</mapper>
